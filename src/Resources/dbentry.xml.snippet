<CodeSnippets
    xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>
        DbEntry
      </Title>
      <Shortcut>dbentry</Shortcut>
      <Author>Lifeng Liang</Author>
      <Description>It will help us to create basic structure of DbEntry to app.config/web.config</Description>
      <HelpUrl>http://www.codeplex.com/dbentry</HelpUrl>
    </Header>
    <Snippet>
      <Code Language="XML">
        <![CDATA[	<configSections>
		<section name="Leafing.Settings" type="Leafing.Core.Setting.NameValueSectionHandler, Leafing.Core" />
	</configSections>

	<Leafing.Settings>
		<add key="DataBase" value="@Access : @~test.mdb" />
	</Leafing.Settings>]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
