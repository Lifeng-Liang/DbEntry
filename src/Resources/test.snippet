<CodeSnippets
    xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>
        Create test case
      </Title>
      <Shortcut>test</Shortcut>
      <Author>Lifeng Liang</Author>
      <Description>It will help us to create test case code</Description>
      <HelpUrl>http://www.codeplex.com/dbentry</HelpUrl>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>TestMethodName</ID>
          <ToolTip>test method name</ToolTip>
          <Default>1</Default>
        </Literal>
      </Declarations>
      <Code Language="CSharp">
        <![CDATA[[Test]
public void Test$TestMethodName$()
{
  $end$
}]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
